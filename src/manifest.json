{
    "manifest_version": 2,
    "name": "Tab Porter",
    "description": "Tab Porter is a free plugin to import/export Tabs from Google Chrome.",
    "version": "1.0.0",
    "icons": { "128": "images/icon_128.png" },
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [ "tabs" ]
}
